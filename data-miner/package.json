{
  "name": "dex-graph",
  "main": "./index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "startM": "CODEBUCKET_ENV=dev ts-node index.ts matic",
    "codegen": "ts-node ./src/scripts/graphcodegen.ts",
    "build": "rimraf dist && npx tsc && ts-node ./src/scripts/deploy.ts",
    "inspect": "node -r ts-node/register --inspect index.ts",
    "check": "tsc --pretty --noEmit",
    "dev": "concurrently \"tsc -w\"  \"npm run start\"",
    "test": "mocha --require ts-node/register src/test/*.spec.ts",
    "prod": "node dist/index.js"
  },
  "dependencies": {
    "@graphql-mesh/transform-prefix": "^0.95.2",
    "aws-sdk": "^2.1441.0",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^2.2.21",
    "@graphprotocol/client-polling-live": "1.1.1",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.1.7",
    "@types/node-cron": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "chai-things": "^0.2.0",
    "compression": "^1.7.4",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "graphql": "16.6.0",
    "mocha": "^10.2.0",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "tsyringe": "^4.7.0",
    "typescript": "^5.0.4",
    "winston": "^3.8.2"
  }
}
